{
  "name": "AutoTools",
  "manifest_version": 2,
  "version": "0.0.1",
  "icons": {
    "16": "./images/icon16.ico"
  },
  "browser_action": {
    "default_icon": "./images/icon16.ico",
    "default_popup": "./popup/popup.html"
  },
  "permissions": [
    "tabs",
    "https://ww16.autotask.net/",
    "storage",
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://ww16.autotask.net/*"
      ],
      "js": ["./extension.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "applications": {
    "gecko": {
      "id": "AutoTools@tamj0rd2.io"
    }
  }
}
